#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules/typedoc/bin/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules/typedoc/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules/typedoc/bin/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules/typedoc/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/typedoc@0.24.8_typescript@5.6.2/node_modules:/mnt/d/Web/Myproject/工程化实战/clone/improve-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typedoc/bin/typedoc" "$@"
else
  exec node  "$basedir/../typedoc/bin/typedoc" "$@"
fi
